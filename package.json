{
  "name": "blackhawks-scouting",
  "version": "2023.2 Prerelease",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "installModules": "npm install",
    "checkUpdates": "ncu -u",
    "generateSchema": "json2ts public/config.schema.json src/config.d.ts"
  },
  "dependencies": {
    "@types/lodash": "^4.14.191",
    "@vueuse/core": "^9.13.0",
    "ajv": "^8.12.0",
    "easyqrcodejs": "^4.6.0",
    "lzma": "^2.3.2",
    "pinia": "^2.0.33",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue-macros/reactivity-transform": "^0.2.5",
    "autoprefixer": "^10.4.14",
    "json-schema-to-typescript": "^12.0.0",
    "npm-check-updates": "^16.7.12",
    "postcss": "^8.4.21",
    "postcss-nested": "^6.0.1",
    "terser": "^5.16.6",
    "typescript": "~5.0.2",
    "vite": "^4.2.0",
    "vite-plugin-live-reload": "3.0.2",
    "vite-plugin-pwa": "^0.14.4"
  },
  "globals": {
    "$$": "readonly",
    "$": "readonly",
    "$computed": "readonly",
    "$customRef": "readonly",
    "$ref": "readonly",
    "$shallowRef": "readonly",
    "$toRef": "readonly",
    "APP_VERSION": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-console": "warn",
    "no-debugger": "warn",
    "quotes": [
      "warn",
      "double",
      {
        "avoidEscape": true
      }
    ]
  }
}
